      },
      "package": "com.vilupule.NetScore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b4ac7d46-cc89-4bf7-ad85-9cd4b08d475f"
